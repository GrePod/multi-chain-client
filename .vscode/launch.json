   "version": "0.2.0",
   "configurations": [
      {
         "type": "pwa-node",
         "request": "launch",
         "name": "Launch Program",
         "skipFiles": ["<node_internals>/**"],
         "program": "${workspaceFolder}/dist/index.js",
         "outFiles": ["${workspaceFolder}/**/*.js"]
      },

      {
         "type": "node",
         "request": "launch",
         "name": "Test mainNetBTC",
         "runtimeExecutable": "yarn",
         "runtimeArgs": ["test", "test/BTC/mainNetBtc.test.ts"]
      },

      {
         "type": "node",
         "request": "launch",
         "console": "integratedTerminal",
         "name": "Test test/BTC/block.test.ts",
         "runtimeExecutable": "yarn",
         "runtimeArgs": ["test", "test/block.test.ts"]
      },

      {
         "type": "node",
         "request": "launch",
         "console": "integratedTerminal",
         "name": "Test test/decoratorTest.test.ts",
         "runtimeExecutable": "yarn",
         "runtimeArgs": ["test", "test/decoratorTest.test.ts"]
      },

      {
         "type": "node",
         "request": "launch",
         "name": "Test transaction ALGO",
         "runtimeExecutable": "yarn",
         "runtimeArgs": ["test", "test/ALGO/transaction.test.ts"]
      },

      {
         "type": "node",
         "request": "launch",
         "name": "Test transaction BTC",
         "runtimeExecutable": "yarn",
         "runtimeArgs": ["test", "test/BTC/transaction.test.ts"]
      },

      {
         "type": "node",
         "request": "launch",
         "name": "Test block DOGE",
         "runtimeExecutable": "yarn",
         "runtimeArgs": ["test", "test/DOGE/block.test.ts"]
      }
   ]
}
